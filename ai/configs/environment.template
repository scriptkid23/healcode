# AI Service Environment Variables Template
# Copy this content to your .env file and configure your values

# Core Configuration
HEALCODE_TENANT_ID=your_tenant_id
HEALCODE_ENVIRONMENT=development
HEALCODE_PRIMARY_MODEL=google_gemini
HEALCODE_LOG_LEVEL=INFO

# AI Model API Keys
GOOGLE_API_KEY=your_google_api_key_here
OPENAI_API_KEY=your_openai_api_key_here  
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Model Specific Settings
HEALCODE_GEMINI_MODEL=gemini-1.5-flash-latest
HEALCODE_GEMINI_TIMEOUT=30.0
HEALCODE_GEMINI_TEMPERATURE=0.7

HEALCODE_OPENAI_MODEL=gpt-4
HEALCODE_OPENAI_TIMEOUT=30.0
HEALCODE_OPENAI_TEMPERATURE=0.7

HEALCODE_ANTHROPIC_MODEL=claude-2
HEALCODE_ANTHROPIC_TIMEOUT=30.0
HEALCODE_ANTHROPIC_TEMPERATURE=0.7

# Redis Configuration
HEALCODE_REDIS_URL=redis://localhost:6379
HEALCODE_REDIS_DB=0
HEALCODE_REDIS_MAX_CONNECTIONS=20

# Zoekt Search Configuration
HEALCODE_ZOEKT_ENDPOINT=http://127.0.0.1:6070/api/search
HEALCODE_ZOEKT_TIMEOUT=10.0
HEALCODE_MAX_CONTEXT_FILES=10

# Cache Configuration
HEALCODE_CACHE_TTL=3600
HEALCODE_CACHE_COMPRESSION=true

# Performance Settings
HEALCODE_MAX_CONCURRENT_REQUESTS=10
HEALCODE_REQUEST_RATE_LIMIT=60

# Security Settings
HEALCODE_MAX_FILE_SIZE=10485760
HEALCODE_ENABLE_AUDIT_LOGGING=true 